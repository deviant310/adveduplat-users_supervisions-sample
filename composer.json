{
    "name": "laravel/laravel",
    "type": "project",
    "version": "0.2.2",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/getcourse-ru/GetCourse-PHP-SDK"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "amocrm/amocrm-api-library": "^0.5.2",
        "doctrine/dbal": "^2.10",
        "dompdf/dompdf": "^1",
        "fideloper/proxy": "^4.2",
        "firebase/php-jwt": "^5.2",
        "fruitcake/laravel-cors": "^2.0",
        "getcourse-ru/getcourse-php-sdk": "dev-master",
        "google/apiclient": "^2.9",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "maatwebsite/excel": "^3.1",
        "mpdf/mpdf": "^8.0",
        "nwidart/laravel-modules": "^8.2",
        "spatie/laravel-activitylog": "^3.17",
        "wpb/string-blade-compiler": "^6.0",
        "zircote/swagger-php": "^3.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.10",
        "crestapps/laravel-code-generator": "^2.4",
        "darkaonline/l5-swagger": "^8.0",
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^3.2",
        "fzaninotto/faker": "^1.9.1",
        "krlove/code-generator": "^1.0",
        "krlove/eloquent-model-generator": "^1.3",
        "laravel/sail": "^1.11",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ]
    }
}
